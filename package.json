{
  "name": "ideal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"json-server --watch db.json --port 3000\" \"ng serve\"",
    "build": "ng build",
    "build:ghpages": "ng run ideal:build-ghpages --base-href /userSaved_angular20/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:ideal": "node dist/ideal/server/server.mjs",
    "deploy:ghpages": "npm run build:ghpages && npx angular-cli-ghpages --dir=dist/ideal/browser"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.0",
    "@angular/common": "^20.2.0",
    "@angular/compiler": "^20.2.0",
    "@angular/core": "^20.2.0",
    "@angular/forms": "^20.2.0",
    "@angular/platform-browser": "^20.2.0",
    "@angular/platform-server": "^20.2.0",
    "@angular/router": "^20.2.0",
    "@angular/ssr": "^20.2.2",
    "@primeng/themes": "^20.1.1",
    "@types/uuid": "^10.0.0",
    "bootstrap": "^5.3.3",
    "chart.js": "^4.5.0",
    "express": "^5.1.0",
    "file-saver": "^2.0.5",
    "primeicons": "^7.0.0",
    "primeng": "^20.1.1",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.23.0",
    "tslib": "^2.3.0",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.2.2",
    "@angular/build": "^20.2.2",
    "@angular/cli": "^20.2.2",
    "@angular/compiler-cli": "^20.2.0",
    "@types/express": "^5.0.1",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "angular-cli-ghpages": "^2.0.1",
    "concurrently": "^9.2.1",
    "jasmine-core": "~5.9.0",
    "json-server": "^1.0.0-beta.3",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
